    <style>
        #loading {
            position: absolute;
            top: 0px;
            bottom: 0px;
            width: 100%;
            z-index: 3;
            background-color: #000000;
        }

        #loading #content {
            position: fixed;
            width: 200px;
            height: 113px;
            top: 25%;
            left: 50%;
            margin-left: -100px;
            background-size: cover;
            background-position: -20px 0;
            z-index: 1;
            background-image: url("data:image/png;base64,");
        }

        #triangle {
            width: 81px;
            height: 85px;
            position: absolute;
            top: 50%;
            left: 50%;
            margin-left: -42px;
            margin-top: -50px;
            z-index: 2;
            background-image: url('data:image/png;base64,');
            animation: loading 1s infinite;
        }

        #progress {
            font-family: "Microsoft YaHei";
            font-size: 20px;
            /* [data-dpr="2"] div {
                    font-size: 32px;
                }*/
            font-weight: 700;
            color: #ffffff;
            position: absolute;
            top: 110%;
            left: 50%;
            transform: translate(-50%, 0);
            z-index: 2;
        }

        @keyframes loading {
            0% {
                top: 47%;
                left: 48%;
            }
            50% {
                top: 49%;
                left: 49%;
            }
            100% {
                top: 48%;
                left: 50%;
            }
        }</style>
